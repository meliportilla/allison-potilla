        public void calcularDoble(){
        int a=16;
        int b=(a+a);
        int c=(a+a+a);
        System.out.println("11. CALCULAR EL DOBLE Y EL TRIPLE DE UN NUMERO: \t");
        System.out.println("1 la respuesta es: ");
        System.out.println(b+"\t");
        System.out.println("2 la respuesta es: ");
        System.out.println(c+"\t");
}
        public void convercionKilometros(){
         int a=16;
        int kilometros=a;
        int metros=(kilometros*1000);
        int centimetros= (kilometros * 100000);
        System.out.println("12. CONVERTIR KILOMETROS A METROS Y CENTIMENTROS: \t ");
        System.out.println("1. "+ kilometros+" kilometros en metros es: "+ metros +" mts"+"\t");

        System.out.println("2. "+ kilometros+" kilometros en centimetros es: "+ centimetros +" cm"+"\t");
}
        public void calcularPerimetro(){                       
        int a= 6;                  
        int lado=a;
        int area=(a*a);
        System.out.println("13. CALCULAR EL AREA Y EL PERIMETRO DE UN CUADRADO \t ");
        System.out.println("el lado es:" +a+"\t ");
        System.out.println("el area del cuadrado es: " + area+"\t");
        System.out.println("el perimetro del cuadrdo:"+ lado* 4 +"\t");
}
         public void calcularVolumen(){      
        int a= 10;
        System.out.println("14. CALCULAR EL VOLUMEN DE UN CUBO\t");           
        System.out.println("el lado del cubo es: "+ a +"\n");
        System.out.println("el volumen es:" + Math.pow(a,3)+"\t");
}

         public void calculaEsfera(){           
        double radio= 5;
        System.out.println("15. CALCULAR EL VOLUMEN DE UNA ESFERA\t");
        String volumen= ((Math.pow(radio,3)3.1416)(4/3)+"\t");
        System.out.println("el radio de la esfera es: " + radio +"\t");
        System.out.println("el volumen es: "+ volumen);
}
        public void sumarNumerosnaturales(){
