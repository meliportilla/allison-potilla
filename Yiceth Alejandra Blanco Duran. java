        public void calcularArea(){
        int c=18;
        double l=3.1416;
        double m=((c*c)*l);
        System.out.println("9. CALCULAR EL AREA DE UN CIRCULO: \t");
        System.out.println("la respuesta es: ");
        System.out.println( m +"\t");
        }

