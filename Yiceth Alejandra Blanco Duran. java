        System.out.println(a+"\t");
}
        public void calcularDescuento(){        
        double a=100;
        double porcentaje= 50;
        System.out.print("17. CALCULAR EL DESCUENYO APLICADO EN UNA COMPRA(PRECIO Y EL PORCENTAJE DE DESCUENTO): \t");                        
        System.out.println("valor del articulo:"+ a +"\t");
        System.out.println("el descuento del articulo es: "+ porcentaje +"\t");

        System.out.println("DESCUENTO APLICADO:"+(a-(a*(porcentaje/100))+"\t");
}

        public void propina() {

        double valor_Total=400;
        double porcentaje_Propina= 10;
        System.out.println("18. CALCULAR LA PROPINA A DEJAR EN UN RESTAURANTE(TOTAL Y PORCENTAJE DESEADO): \t");
        System.out.println("valor Total:"+ valor_Total +" pesos \t");
        System.out.println("El porcentaje de la propina es: "+ porcentaje_Propina +" %\t");
        System.out.println("Valor de la Propina:"+(valor_Total*(porcentaje_Propina/100))+" pesos\t");
        }

